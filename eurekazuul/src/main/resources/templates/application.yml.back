server:
  port: 8085
spring:
  application:
    name: eureka-zull
eureka:
  client:
    service-url:
      defaultZone: http://${eureka.instance.hostname}:8761/eureka
  instance:
    hostname: localhost
    prefer-ip-address: true
zuul:
  routes:
    routes:
        abc:
          path: /client/**
          serviceId: eureka-client